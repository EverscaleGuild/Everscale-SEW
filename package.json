{
  "name": "everscale-sew",
  "version": "1.0.0-alpha",
  "description": "Cryptographic game SEW (Slashing, EVER, Whitepaper) - in fact it is rock, scissors, paper (ro-sham-bo), the game process is controlled via NFT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EverscaleGuild/Everscale-SEW.git"
  },
  "keywords": [
    "nft",
    "tvm",
    "solidity",
    "everscale"
  ],
  "author": "EverscaleGuild",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EverscaleGuild/Everscale-SEW/issues"
  },
  "homepage": "https://github.com/EverscaleGuild/Everscale-SEW#readme",
  "scripts": {
    "nvm": "echo $npm_package_engines_node | cut -d'=' -f2",
    "postinstall": "yarn setup-everdev",
    "se-local-reset": "everdev se reset",
    "setup-everdev": "everdev sol set --compiler 0.57.3 --linker 0.14.39",
    "build-contracts": "locklift build --config locklift.config.js",
    "contract-deploy-local": "yarn se-local-reset && locklift run --config locklift.config.js --network local --script scripts/deploy.js",
    "contract-deploy-dev": "locklift run --config locklift.config.js --network dev --script scripts/deploy.js",
    "test-contracts": "yarn se-local-reset && locklift test --config locklift.config.js --network local",
    "test": "yarn test-contracts"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@broxus/contracts": "EverscaleGuild/broxus-contracts",
    "chai": "^4.3.6",
    "everdev": "^1.1.1",
    "locklift": "ilyar/ton-locklift"
  }
}
